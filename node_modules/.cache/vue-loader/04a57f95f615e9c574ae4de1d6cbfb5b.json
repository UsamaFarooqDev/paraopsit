                 <div class=\"col-sm-6\">\n                               \n                                <select class=\"form-control ipf\" required name=\"service\">\n                                    <option value=\"Select Service\">Select Service  <i class=\"fas fa-arrow-circle-down\"></i></option>\n                                    <option value=\"Web Development\">Web Development</option>\n                                    <option value=\"App Development\">App Development</option>\n                                    <option value=\"Graphics Design\">Graphics Design</option>\n                                    <option value=\"Social Media Marketing\">Social Media Marketing</option>\n                                    <option value=\"SEO & Content Writting\">SEO & Content Writting</option>\n                                </select>\n                            </div>\n                        </div>\n                        <br /> <br />\n                        <div class=\"row\">\n                            <div class=\"col-sm-12\">\n                                <textarea name=\"msg\" v-model=\"message\" required placeholder=\"Message\" class=\"form-control ipf\" id=\"\" cols=\"30\" rows=\"5\"></textarea>\n                            </div>\n                        </div>\n                        <br /><br />\n                        <div class=\"row\">\n                            <div class=\"col-sm-12 text-center\">\n                                <input type=\"submit\" value=\"Submit\" class=\"btn btn-dark ipb\">\n                            </div>\n                        </div>\n                    </form>\n\n                </div>\n            </div>\n        </div>\n        <hr>\n</template>\n<script>\nimport emailjs from 'emailjs-com';\nexport default {\n     mounted () {\n  window.scrollTo(0, 0)\n},\n    name: 'getquote',\n      data() {\n    return {\n      name: '',\n      email: '',\n      phone:'',\n      service: '',\n      message: ''\n    }\n  },\n  methods: {\n    sendEmail(e) {\n      try {\n        emailjs.sendForm('service_hcsus1k', 'template_jkxeaov', e.target,\n        'user_NzjQHvajhc1V6sNxAflyP', {\n          name: this.name,\n          email: this.email,\n          phone: this.phone,\n          service: this.service,\n          message: this.message\n        })\n\n      } catch(error) {\n          console.log({error})\n      }\n      // Reset form field\n      this.name = ''\n      this.email = ''\n      this.phone = ''\n      this.service = ''\n      this.message = ''\n    },\n  }\n}\n</script>\n<style scoped>\n.form-contain h1{\n    color: white;\n}\n.main-text h1{\n    margin-top: 200px;\n}\nimg{\n    height: 700px;\n    width: 100%;\n    background-size: cover;\n}\n.ipf{\n    background-color: transparent !important;\n    color: white !important;\n    margin-bottom: 15px;\n    border-radius: 0px !important;\n    border: none !important;\n    border-bottom: 1px solid white !important;\n}\nselect{\n    color: white !important;\n}\n.ipf option{\n    background-color: black;\n}\n.ipf:focus{\n    box-shadow: none;\n}\n::placeholder{\n    color: white;\n}\n.ipb{\n    background-color: transparent;\n    color: white !important;\n    font-weight: 500;\n    border-radius: 0px;\n    padding: 10px;\n    border: 2px solid white;\n}\n.form-contain{\n      background-color: #024141bd;\n    padding:20px;\n}\n#banner{\n    height: 600px;\n}\n</style>"]}]}